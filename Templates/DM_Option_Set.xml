<?xml version="1.0" encoding="UTF-8"?>
<DeploymentOptions version="5.2.1">
	<ImportOptions>
        <Environment>environment_name</Environment>
        <DeployDataSet>deploy_dataset_name</DeployDataSet>

        <UpdateOption>UpdateAlways</UpdateOption>
        <CreateOption>CreateNotAllowed</CreateOption>
        <WorkflowConfigurationOption>Overwrite</WorkflowConfigurationOption>

        <ImportSecurity value="false"/>
        <ImportOwner value="false"/>
        <ImportObjectId value="false"/>
        <UseOriginalTimestamps value="false"/>
        <ImportRetention value="false"/>
        <RemovePropertyDefinitions value="false"/>
        <TransferWorkflows value="true"/>

        <AuditOption>AuditOnly</AuditOption>

        <DeleteCreatedFilesOnError value="false"/>
        <ReportFileName>AuditFile</ReportFileName>

        <UpdateLocalizedProperties value="true"/>

        <StoragePolicy type="PropertyOnExportedObject"></StoragePolicy>

	</ImportOptions>
</DeploymentOptions>